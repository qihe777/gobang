<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wuziqi">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/tubiao"

        android:label="欢乐五子棋"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="service.MyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".playactivity.AiToAiActivity" />
        <activity android:name=".playactivity.HumenToAiActivity" />
        <activity android:name=".playactivity.HumenWebActivity" />
        <activity android:name=".HallActivity" />
    </application>

</manifest>
